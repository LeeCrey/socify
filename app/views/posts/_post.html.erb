<div id="<%= dom_id post %>">
  <p>
    <strong>User:</strong>
    <%= post.user_id %>
  </p>

  <p>
    <strong>Content:</strong>
    <%= post.content %>
  </p>

  <p>
    <strong>Photos:</strong>
    <% post.photos.each do |photo| %>
      <div><%= link_to photo.filename, photo %></div>
    <% end %>
  </p>

</div>
